#!/source/this/sh
export MYBORG=$(cat ~/.local/var/myborg/current.txt)
echo "++[$MYBORG/main] $*"

. ${MYBORG}/CONSTANTS

mkdir -p $MYBORG_BUILD
mkdir -p $MYBORG_MODS
touch $MYBORG_BUILD/.bch
touch $MYBORG_MODS/.bch

${MYBORG}/MANIFEST

. myborg:lib
myborg.lbin.load
myborg.lbin.clean
__myborg_mods_activate
__myborg_borgify $*
export PATH=$(myborg.path clean)


echo "++[$MYBORG/main] $*"
