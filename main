#!/source/this/sh
export MYBORG=$(cat ~/.local/var/myborg/current.txt)
export PATH=${PATH}:${MYBORG}/bin

[ "$MYPID" = "" ] && export MYPID=$$

. myborg:constants
. myborg:activate

[ "$MYPID" = $$ ] && . myborg:first
[ "$MYPID" = $$ ] || . myborg:second

. ${MYBORG_DOTS}/dot$1

echo ENTER LEVEL $(( ${SHLVL} - 1 ))
trap "echo ENTER LEVEL $(( ${SHLVL} - 2 ))" 0

export_ PATH    $(myborg.path clean)
alias myborg="pushd ${MYBORG}"

