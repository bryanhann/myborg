#!/source/this/sh
export MYBORG=$(cat ~/.local/var/myborg/current.txt)
. $MYBORG/CONSTANTS
[ "$MYPID" == "" ] && export MYPID=$$
[ $MYPID = $$ ] && . myborg:install
[ $MYPID = $$ ] || . myborg:activate
export PATH=$(myborg.path clean)

[ "$MYPID" = "$$" ] && {
    exit () { echo try \'builtin exit\' ; }
 }||{
    true;
 }

. ${MYBORG_DOTS}/dot$1
echo ENTER LEVEL $(( ${SHLVL} - 1 ))
trap "echo ENTER LEVEL $(( ${SHLVL} - 2 ))" 0
